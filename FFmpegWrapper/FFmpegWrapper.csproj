<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<Nullable>enable</Nullable>
		<Description>A ffmpeg wrapper that uses a fluent api to work with</Description>
		<Version>0.0.2.8</Version>
		<AssemblyName>FFmpegWrapper</AssemblyName>
		<RootNamespace>FFmpegWrapper</RootNamespace>
	</PropertyGroup>


	<ItemGroup>
		<PackageReference Include="NEbml" Version="0.11.0" />
	</ItemGroup>


	<ItemGroup>
		<Compile Update="Models\FFProcess.cs">
			<SubType>Component</SubType>
		</Compile>
	</ItemGroup>


	<ItemGroup>
		<None Update="ffmpeg.exe">
			<CopyToOutputDirectory Condition="Exists('$(MSBuildProjectDirectory)\ffmpeg.exe')">Always</CopyToOutputDirectory>
		</None>
		<None Update="ffprobe.exe">
			<CopyToOutputDirectory Condition="Exists('$(MSBuildProjectDirectory)\ffprobe.exe')">Always</CopyToOutputDirectory>
		</None>
	</ItemGroup>

</Project>
